{
  "name": "@nounou/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "format": "npx prettier --write \"{app,libs}/**/src/**/*.{ts,html,css}\"",
    "lint-staged": "npx lint-staged",
    "prepare": "husky",
    "express-api-dev": "npx nx serve express-api --no-inspect",
    "migrate:nx:check:latest": "npx nx migrate latest",
    "migrate:nx:apply": "npx nx migrate --run-migrations",
    "install:standart": "npm install",
    "install:force": "npm install --force",
    "install:legacy-peer-deps": "npm install --legacy-peer-deps"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ]
  },
  "private": true,
